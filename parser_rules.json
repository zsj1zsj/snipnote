      ],
      "stop_patterns": [
        "^\\s*Comments\\s*$",
        "^\\s*oauth comment\\s*$",
        "^\\s*footer\\s*$"
      ],
      "image_drop_keywords": ["logo", "icon", "avatar", "favicon", "qlogo", "qrcode"],
      "max_images": 3
    }
  }
}
