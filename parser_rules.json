      "max_images": 2,
      "min_blocks": 2,
      "min_blocks_error": "未提取到论坛帖正文。"
    },
    "liaoxuefeng": {
      "domains": ["liaoxuefeng.com", "www.liaoxuefeng.com"],
      "primary_container_ids": ["gsi-chapter-content", "gsi-chapter"],
      "primary_html_patterns": [
        "(?is)<div[^>]*id=[\"']?gsi-chapter-content[\"']?[^>]*>(.*?)</div>",
        "(?is)<div[^>]*id=[\"']?gsi-chapter[\"']?[^>]*>(.*?)</div>",
        "(?is)<main[^>]*>(.*?)</main>"
      ],
      "drop_patterns": [
        "^\\s*Comments\\s*$",
        "^\\s*Loading comments\\.\\.\\.\\s*$",
        "^\\s*Fail to load comments\\..*$",
        "^\\s*Comments loaded\\..*$",
        "^\\s*To post a comment, please\\s*$",
        "^\\s*Sign In\\s*$",
        "^\\s*Sign Out\\s*$",
        "^\\s*Read More\\s*$",
        "^\\s*Collapse\\s*$",
        "^\\s*上一页\\s*$",
        "^\\s*下一页\\s*$",
        "^\\s*泛型\\s*$",
        "^\\s*使用泛型\\s*$",
